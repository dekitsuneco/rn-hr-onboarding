{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@store": [
        "src/app/shared/store"
      ],
      "@store/*": [
        "src/app/shared/store/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@styles": [
        "src/styles"
      ],
      "@tests/*": [
        "src/tests/*"
      ],
      "@app/*": [
        "src/app/*"
      ]
    }
  },
  "extends": "../../tsconfig.json"
}
